<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\CommonData\TargetFrameworks.props" />

    <PropertyGroup>
      <OutputType>WinExe</OutputType>
      <Platforms>AnyCPU;x64;x86</Platforms>
      <ApplicationManifest>app.manifest</ApplicationManifest>
      <LangVersion>11.0</LangVersion>
      <Nullable>enable</Nullable>
      <ApplicationIcon>../CommonData/Sample.ico</ApplicationIcon>
    </PropertyGroup>


	<ItemGroup>
      <Compile Remove="build\**" />
      <EmbeddedResource Remove="build\**" />
      <None Remove="build\**" />
    </ItemGroup>

    <ItemGroup Condition="$(DefineConstants.Contains(';NETFRAMEWORK'))">
        <Reference Include="Microsoft.CSharp" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="AlignmentControl.uixml" />
      <None Remove="FocusWindow.uixml" />
      <None Remove="Grid10x10Window.uixml" />
      <None Remove="GridLayoutPropertiesWindow.uixml" />
      <None Remove="GridLengthControl.uixml" />
      <None Remove="Resources\logo.png" />
      <None Remove="StackLayoutPropertiesWindow.uixml" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Include="Resources\logo.png" />
    </ItemGroup>

    <ItemGroup>
		<ProjectReference Include="..\..\Alternet.UI\Alternet.UI.csproj" />
	</ItemGroup>
</Project>
