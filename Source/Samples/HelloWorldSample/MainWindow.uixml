        x:Class="HelloWorldSample.MainWindow"
        Title="Alternet UI Hello World Sample" StartLocation="CenterScreen"
        xmlns:drawing="clr-namespace:Alternet.Drawing;assembly=Alternet.UI"
        MinWidth="500" MinHeight="200">
