<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="scheme.xsd">
  <Author>AlterNET Software</Author>
  <Name>Visual Basic language</Name>
  <Desc>Syntax Scheme for Visual Basic Language</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>.vb</FileExtension>
  <FileType>vbasic</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>0</Index>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <Index>1</Index>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <Index>2</Index>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Silver</ForeColor>
      <FontStyle>Italic</FontStyle>
      <Index>3</Index>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <Index>4</Index>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <Index>5</Index>
    </Style>
    <Style>
      <Name>directives</Name>
      <ForeColor>Blue</ForeColor>
      <Index>6</Index>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>directive</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\#.*</string>
          </Expressions>
          <Index>0</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment1</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\'.*</string>
            <string>rem[\u0000-\u0020].*</string>
          </Expressions>
          <Index>1</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <Index>0</Index>
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>string</word>
                <word>unlock</word>
                <word>do</word>
                <word>else</word>
                <word>withevents</word>
                <word>filecopy</word>
                <word>explicit</word>
                <word>width</word>
                <word>rmdir</word>
                <word>while</word>
                <word>return</word>
                <word>true</word>
                <word>date</word>
                <word>end</word>
                <word>setattr</word>
                <word>load</word>
                <word>optional</word>
                <word>loop</word>
                <word>name</word>
                <word>appactivate</word>
                <word>mod</word>
                <word>sendkeys</word>
                <word>stop</word>
                <word>put</word>
                <word>next</word>
                <word>select</word>
                <word>deftype</word>
                <word>then</word>
                <word>chdrive</word>
                <word>is</word>
                <word>get</word>
                <word>sub</word>
                <word>type</word>
                <word>mid</word>
                <word>byref</word>
                <word>enum</word>
                <word>write</word>
                <word>set</word>
                <word>if</word>
                <word>as</word>
                <word>compare</word>
                <word>static</word>
                <word>rset</word>
                <word>empty</word>
                <word>raiseevent</word>
                <word>input</word>
                <word>const</word>
                <word>savepicture</word>
                <word>case</word>
                <word>eqv</word>
                <word>binary</word>
                <word>seek</word>
                <word>lset</word>
                <word>step</word>
                <word>gosub</word>
                <word>resume</word>
                <word>goto</word>
                <word>imp</word>
                <word>chdir</word>
                <word>exit</word>
                <word>dim</word>
                <word>error</word>
                <word>lock</word>
                <word>base</word>
                <word>savesetting</word>
                <word>like</word>
                <word>xor</word>
                <word>to</word>
                <word>redim</word>
                <word>public</word>
                <word>beep</word>
                <word>time</word>
                <word>property</word>
                <word>len</word>
                <word>call</word>
                <word>randomize</word>
                <word>wend</word>
                <word>print</word>
                <word>not</word>
                <word>unload</word>
                <word>addressof</word>
                <word>implements</word>
                <word>private</word>
                <word>line</word>
                <word>friend</word>
                <word>mkdir</word>
                <word>nothing</word>
                <word>new</word>
                <word>with</word>
                <word>close</word>
                <word>event</word>
                <word>deletesetting</word>
                <word>me</word>
                <word>declare</word>
                <word>reset</word>
                <word>on</word>
                <word>paramarray</word>
                <word>function</word>
                <word>false</word>
                <word>null</word>
                <word>for</word>
                <word>let</word>
                <word>open</word>
                <word>kill</word>
                <word>and</word>
                <word>option</word>
                <word>byval</word>
                <word>or</word>
                <word>each</word>
                <word>erase</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
          <Index>2</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|([0-9]+)</string>
          </Expressions>
          <Index>3</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
          <Index>4</Index>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)*</string>
          </Expressions>
          <Index>5</Index>
        </SyntaxBlock>
      </SyntaxBlocks>
      <Index>0</Index>
    </State>
  </States>
</LexScheme>