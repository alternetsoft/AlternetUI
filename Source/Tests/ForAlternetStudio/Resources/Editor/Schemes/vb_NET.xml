<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>AlterNET Software</Author>
  <Name>Visual Basic.NET language</Name>
  <Desc>Syntax Scheme for VB.Net Language</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>vb</FileExtension>
  <FileType>.vb</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Silver</ForeColor>
      <FontStyle>Italic</FontStyle>
      <PlainText>true</PlainText>
    </Style>
    <Style>
      <Name>whitespace</Name>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
    </Style>
    <Style>
      <Name>directives</Name>
      <ForeColor>Blue</ForeColor>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>directive</Name>
          <LexStyle>6</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name>ResWord Set0</Name>
              <ResWordStyle>6</ResWordStyle>
              <ResWords>
                <word>#Region</word>
                <word>#End Region</word>
                <word>#ElseIf</word>
                <word>#End If/String</word>
                <word>#If</word>
                <word>#Else</word>
                <word>#error</word>
                <word>#Const</word>
                <word>#ExternalSource</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>\#.*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comment1</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\'.*</string>
            <string>rem[\u0000-\u0020].*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>try</word>
                <word>catch</word>
                <word>finally</word>
                <word>when</word>
                <word>inherits</word>
                <word>overloads</word>
                <word>readonly</word>
                <word>writeonly</word>
                <word>default</word>
                <word>notinheritable</word>
                <word>mustinherit</word>
                <word>implements</word>
                <word>myclass</word>
                <word>mybase</word>
                <word>protected</word>
                <word>friend</word>
                <word>public</word>
                <word>private</word>
                <word>shared</word>
                <word>overrides</word>
                <word>overridable</word>
                <word>namespace</word>
                <word>string</word>
                <word>unlock</word>
                <word>do</word>
                <word>else</word>
                <word>withevents</word>
                <word>filecopy</word>
                <word>explicit</word>
                <word>width</word>
                <word>rmdir</word>
                <word>while</word>
                <word>return</word>
                <word>true</word>
                <word>date</word>
                <word>end</word>
                <word>setattr</word>
                <word>load</word>
                <word>optional</word>
                <word>loop</word>
                <word>name</word>
                <word>appactivate</word>
                <word>mod</word>
                <word>sendkeys</word>
                <word>stop</word>
                <word>put</word>
                <word>next</word>
                <word>select</word>
                <word>deftype</word>
                <word>then</word>
                <word>chdrive</word>
                <word>is</word>
                <word>get</word>
                <word>sub</word>
                <word>type</word>
                <word>mid</word>
                <word>byref</word>
                <word>enum</word>
                <word>write</word>
                <word>set</word>
                <word>if</word>
                <word>structure</word>
                <word>delegate</word>
                <word>as</word>
                <word>compare</word>
                <word>static</word>
                <word>rset</word>
                <word>empty</word>
                <word>raiseevent</word>
                <word>input</word>
                <word>const</word>
                <word>savepicture</word>
                <word>case</word>
                <word>eqv</word>
                <word>binary</word>
                <word>seek</word>
                <word>lset</word>
                <word>step</word>
                <word>gosub</word>
                <word>resume</word>
                <word>goto</word>
                <word>imp</word>
                <word>chdir</word>
                <word>exit</word>
                <word>dim</word>
                <word>error</word>
                <word>lock</word>
                <word>base</word>
                <word>savesetting</word>
                <word>like</word>
                <word>xor</word>
                <word>to</word>
                <word>redim</word>
                <word>beep</word>
                <word>time</word>
                <word>property</word>
                <word>len</word>
                <word>call</word>
                <word>randomize</word>
                <word>wend</word>
                <word>print</word>
                <word>not</word>
                <word>unload</word>
                <word>addhandler</word>
                <word>alias</word>
                <word>andalso</word>
                <word>ansi</word>
                <word>assembly</word>
                <word>auto</word>
                <word>boolean</word>
                <word>byte</word>
                <word>cbool</word>
                <word>cbyte</word>
                <word>cchar</word>
                <word>cdate</word>
                <word>cdec</word>
                <word>cdbl</word>
                <word>char</word>
                <word>cint</word>
                <word>clng</word>
                <word>cobj</word>
                <word>cshort</word>
                <word>csng</word>
                <word>cstr</word>
                <word>ctype</word>
                <word>decimal</word>
                <word>directcast</word>
                <word>double</word>
                <word>elseif</word>
                <word>gettype</word>
                <word>handles</word>
                <word>in</word>
                <word>integer</word>
                <word>interface</word>
                <word>lib</word>
                <word>long</word>
                <word>module</word>
                <word>mustoverride</word>
                <word>notoverridable</word>
                <word>object</word>
                <word>orelse</word>
                <word>preserve</word>
                <word>removehandler</word>
                <word>shadows</word>
                <word>short</word>
                <word>single</word>
                <word>synclock</word>
                <word>throw</word>
                <word>typeof</word>
                <word>unicode</word>
                <word>until</word>
                <word>variant</word>
                <word>addressof</word>
                <word>class</word>
                <word>imports</word>
                <word>strict</word>
                <word>line</word>
                <word>friend</word>
                <word>mkdir</word>
                <word>nothing</word>
                <word>new</word>
                <word>with</word>
                <word>close</word>
                <word>event</word>
                <word>deletesetting</word>
                <word>me</word>
                <word>declare</word>
                <word>reset</word>
                <word>off</word>
                <word>on</word>
                <word>paramarray</word>
                <word>function</word>
                <word>false</word>
                <word>null</word>
                <word>for</word>
                <word>let</word>
                <word>open</word>
                <word>kill</word>
                <word>and</word>
                <word>option</word>
                <word>byval</word>
                <word>or</word>
                <word>each</word>
                <word>erase</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|([0-9]+)</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>string</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)*</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
  </States>
</LexScheme>