<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>AlterNET Software</Author>
  <Name>PowerShell language</Name>
  <Desc>Syntax Scheme for PowerShell Language</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>.ps1</FileExtension>
  <FileType>powershell</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>ControlText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Blue</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Green</ForeColor>
      <PlainText>true</PlainText>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>0</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
	  <Style>
		  <Name>commands</Name>
		  <ForeColor>Maroon</ForeColor>
		  <ForeColorEnabled>true</ForeColorEnabled>
		  <BackColorEnabled>true</BackColorEnabled>
		  <BoldEnabled>true</BoldEnabled>
		  <ItalicEnabled>true</ItalicEnabled>
		  <UnderlineEnabled>true</UnderlineEnabled>
	  </Style>
	  <Style>
		  <Name>parameters</Name>
		  <ForeColor>Blue</ForeColor>
		  <ForeColorEnabled>true</ForeColorEnabled>
		  <BackColorEnabled>true</BackColorEnabled>
		  <BoldEnabled>true</BoldEnabled>
		  <ItalicEnabled>true</ItalicEnabled>
		  <UnderlineEnabled>true</UnderlineEnabled>
	  </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                  <word>begin</word>
				  <word>break</word>
				  <word>catch</word>
				  <word>class</word>
				  <word>continue</word>
				  <word>data</word>
				  <word>define</word>
				  <word>do</word>
				  <word>dynamicparam</word>
				  <word>else</word>
				  <word>elseif</word>
				  <word>end</word>
				  <word>exit</word>
				  <word>filter</word>
				  <word>finally</word>
				  <word>for</word>
				  <word>foreach</word>
				  <word>from</word>
				  <word>function</word>
				  <word>if</word>
				  <word>in</word>
				  <word>inlinescript</word>
				  <word>parallel</word>
				  <word>param</word>
				  <word>process</word>
				  <word>return</word>
				  <word>switch</word>
				  <word>throw</word>
				  <word>trap</word>
				  <word>try</word>
				  <word>until</word>
				  <word>using</word>
				  <word>var</word>
				  <word>while</word>
				  <word>workflow</word>
			  </ResWords>
            </ResWordSet>
			  <ResWordSet>
				  <Name />
				  <ResWordStyle>6</ResWordStyle>
				  <ResWords>
					  <word>Set-Location</word>
					  <word>Get-Content</word>
					  <word>Add-Content</word>
					  <word>Add-PSSnapIn</word>
					  <word>Set-Content</word>
					  <word>Copy-Item</word>
					  <word>Copy-ItemProperty</word>
					  <word>Remove-Item</word>
					  <word>Move-Item</word>
					  <word>Set-Item</word>
					  <word>New-Item</word>
					  <word>Start-Job</word>
					  <word>Compare-Object</word>
					  <word>Group-Object</word>
					  <word>Invoke-WebRequest</word>
					  <word>Measure-Object</word>
					  <word>Resolve-Path</word>
					  <word>Resume-Job</word>
					  <word>Set-Variable</word>
					  <word>Show-Command</word>
					  <word>Sort-Object</word>
					  <word>Start-Service</word>
					  <word>Start-Process</word>
					  <word>Suspend-Job</word>
					  <word>Wait-Job</word>
					  <word>Where-Object</word>
					  <word>Write-Output</word>
					  <word>Get-Command</word>
					  <word>Get-Help</word>
					  <word>Set-ExecutionPolicy</word>
					  <word>Get-Service</word>
					  <word>ConvertTo-HTML</word>
					  <word>Get-EventLog</word>
					  <word>Get-Process</word>
					  <word>Clear-History</word>
					  <word>Set-AuthenticodeSignature</word>
					  <word>ForEach-Object</word>
					  <word>Clear-Content</word>
					  <word>Clear-Host</word>
					  <word>Clear-History</word>
					  <word>Clear-Item</word>
					  <word>Clear-ItemProperty</word>
					  <word>Clear-Variable</word>
					  <word>Checkpoint-Computer</word>
					  <word>ConvertFrom-StringData</word>
					  <word>ConvertTo-SecureString</word>
					  <word>ConvertTo-XML</word>
					  <word>Format-List</word>
					  <word>Format-Table</word>
					  <word>ForEach-Object</word>
					  <word>Format-Wide</word>
					  <word>Get-Alias</word>
					  <word>New-AppLockerPolicy</word>
					  <word>New-ItemProperty</word>
					  <word>New-Object</word>
					  <word>New-WebServiceProxy</word>
					  <word>New-WSManInstance</word>
					  <word>New-WSManSessionOption</word>
					  <word>Select-Object</word>
					  <word>Set-Alias</word>
					  <word>Set-StrictMode</word>
					  <word>Write-Progress</word>
					  <word>Debug-Process</word>
					  <word>Disable-PSBreakpoint</word>
					  <word>Get-Counter</word>
					  <word>Export-Counter</word>
					  <word>Test-Path</word>
					  <word>Get-WinEvent</word>
					  <word>Invoke-TroubleshootingPack</word>
					  <word>Measure-Command</word>
					  <word>New-Event</word>
					  <word>Receive-Job</word>
					  <word>Register-EngineEvent</word>
					  <word>Register-ObjectEvent</word>
					  <word>Remove-Event</word>
					  <word>Set-PSDebug</word>
					  <word>Start-Sleep</word>
					  <word>Tee-Object</word>
					  <word>Test-AppLockerPolicy</word>
					  <word>Test-ComputerSecureChannel</word>
					  <word>Trace-Command</word>
					  <word>Write-Debug</word>
				  </ResWords>
			  </ResWordSet>
		  </ResWordSets>
          <Expressions>
            <string>[\$a-zA-Z_][\{\}\-a-zA-Z0-9_]*</string>
          </Expressions>
        </SyntaxBlock>
		  <SyntaxBlock>
			  <Name>parameters</Name>
			  <LexStyle>7</LexStyle>
			  <LeaveState>0</LeaveState>
			  <Expressions>
				  <string>[\-][\-a-zA-Z0-9_]*</string>
			  </Expressions>
		  </SyntaxBlock>
		  <SyntaxBlock>
          <Name>comment1</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\#.*</string>
          </Expressions>
        </SyntaxBlock>
		  <SyntaxBlock>
			  <Name>comment2</Name>
			  <LexStyle>3</LexStyle>
			  <LeaveState>1</LeaveState>
			  <Expressions>
				  <string>&lt;\#</string>
			  </Expressions>
		  </SyntaxBlock>
		  <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|(0(x|X)[0-9a-fA-F]+)|([0-9]+)</string>
          </Expressions>
        </SyntaxBlock>
		  <SyntaxBlock>
			  <Name>string1</Name>
			  <LexStyle>5</LexStyle>
			  <LeaveState>0</LeaveState>
			  <Expressions>
				  <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
			  </Expressions>
		  </SyntaxBlock>
		  <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
	  <State>
		  <Name>comment</Name>
		  <CaseSensitive>true</CaseSensitive>
		  <SyntaxBlocks>
			  <SyntaxBlock>
				  <Name>comment21</Name>
				  <LexStyle>3</LexStyle>
				  <LeaveState>0</LeaveState>
				  <Expressions>
					  <string>\#&gt;</string>
				  </Expressions>
			  </SyntaxBlock>
			  <SyntaxBlock>
				  <Name>comment22</Name>
				  <LexStyle>3</LexStyle>
				  <LeaveState>1</LeaveState>
				  <Expressions>
					  <string>((?!\#&gt;).)+</string>
				  </Expressions>
			  </SyntaxBlock>
		  </SyntaxBlocks>
	  </State>
  </States>
</LexScheme>