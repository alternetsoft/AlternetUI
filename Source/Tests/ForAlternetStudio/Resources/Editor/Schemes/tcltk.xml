<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>AlterNET Software</Author>
  <Name>TclTk language</Name>
  <Desc>Syntax Scheme for TclTk Language</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>.tcl</FileExtension>
  <FileType>tcltk</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>Green</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Navy</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Gray</ForeColor>
      <FontStyle>Italic</FontStyle>
      <PlainText>true</PlainText>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>0</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>focus</word>
                <word>catch</word>
                <word>entry</word>
                <word>listbox</word>
                <word>if</word>
                <word>package</word>
                <word>global</word>
                <word>set</word>
                <word>return</word>
                <word>message</word>
                <word>tkvars</word>
                <word>break</word>
                <word>append</word>
                <word>list</word>
                <word>string</word>
                <word>bind</word>
                <word>glob</word>
                <word>http</word>
                <word>options</word>
                <word>foreach</word>
                <word>rgb</word>
                <word>library</word>
                <word>image</word>
                <word>lindex</word>
                <word>font</word>
                <word>grid</word>
                <word>history</word>
                <word>lsearch</word>
                <word>tk_getsavefile</word>
                <word>while</word>
                <word>llength</word>
                <word>pid</word>
                <word>lower</word>
                <word>else</word>
                <word>concat</word>
                <word>selection</word>
                <word>uplevel</word>
                <word>unknown</word>
                <word>canvas</word>
                <word>interp</word>
                <word>proc</word>
                <word>fblocked</word>
                <word>expr</word>
                <word>tk_bisque</word>
                <word>open</word>
                <word>checkbutton</word>
                <word>binary</word>
                <word>frame</word>
                <word>rename</word>
                <word>grab</word>
                <word>format</word>
                <word>cd</word>
                <word>regsub</word>
                <word>seek</word>
                <word>tk_setpalette</word>
                <word>tclvars</word>
                <word>namespace</word>
                <word>then</word>
                <word>bgerror</word>
                <word>tk_popup</word>
                <word>pack</word>
                <word>trade</word>
                <word>read</word>
                <word>incr</word>
                <word>tk_choosecolor</word>
                <word>bindproc</word>
                <word>resource</word>
                <word>text</word>
                <word>pegexp</word>
                <word>lsort</word>
                <word>fconfigure</word>
                <word>bitmap</word>
                <word>palce</word>
                <word>linsert</word>
                <word>close</word>
                <word>tk_getopenfile</word>
                <word>info</word>
                <word>kt_messagebox</word>
                <word>subst</word>
                <word>tk_dialog</word>
                <word>error</word>
                <word>tk_focusfollowsmouse</word>
                <word>filename</word>
                <word>radiobutton</word>
                <word>lreplace</word>
                <word>lappend</word>
                <word>scale</word>
                <word>button</word>
                <word>wm</word>
                <word>load</word>
                <word>fileevent</word>
                <word>exit</word>
                <word>continue</word>
                <word>variable</word>
                <word>socket</word>
                <word>switch</word>
                <word>tcl</word>
                <word>array</word>
                <word>pwd</word>
                <word>registry</word>
                <word>winfo</word>
                <word>send</word>
                <word>flush</word>
                <word>bindidproc</word>
                <word>update</word>
                <word>destroy</word>
                <word>puts</word>
                <word>split</word>
                <word>file</word>
                <word>toplevel</word>
                <word>time</word>
                <word>clipboard</word>
                <word>tk</word>
                <word>safebase</word>
                <word>vwait</word>
                <word>menu</word>
                <word>option</word>
                <word>label</word>
                <word>bell</word>
                <word>eof</word>
                <word>tkerror</word>
                <word>unset</word>
                <word>join</word>
                <word>namespupd</word>
                <word>gets</word>
                <word>after</word>
                <word>eval</word>
                <word>raise</word>
                <word>lrange</word>
                <word>photo</word>
                <word>event</word>
                <word>tk_focusnext</word>
                <word>tk_optionmenu</word>
                <word>sendout</word>
                <word>exec</word>
                <word>upvar</word>
                <word>loadtk</word>
                <word>scan</word>
                <word>clock</word>
                <word>bindtags</word>
                <word>tkwait</word>
                <word>source</word>
                <word>tk_focusprev</word>
                <word>fcopy</word>
                <word>for</word>
                <word>prg_mkindex</word>
                <word>tell</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>comments</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)*\#.*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|([0-9]+)</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>strings</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"((((\\")|("")|[^"])*")|(((\\")|("")|[^"])*))</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
  </States>
</LexScheme>