<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>AlterNET Software</Author>
  <Name>Batch language</Name>
  <Desc>Syntax Scheme for MS-DOS Batch Language</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>.bat</FileExtension>
  <FileType>batch</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>ControlText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>numbers</Name>
      <ForeColor>Green</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>reswords</Name>
      <ForeColor>Navy</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Gray</ForeColor>
      <PlainText>true</PlainText>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>WindowText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>strings</Name>
      <ForeColor>Maroon</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comments</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\:.*</string>
            <string>rem[\u0000-\u0020].*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <ResWordSets>
            <ResWordSet>
              <Name />
              <ResWordStyle>2</ResWordStyle>
              <ResWords>
                <word>call</word>
                <word>do</word>
                <word>echo</word>
                <word>errorlevel</word>
                <word>exist</word>
                <word>for</word>
                <word>goto</word>
                <word>if</word>
                <word>in</word>
                <word>not</word>
                <word>pause</word>
                <word>set</word>
                <word>shift</word>
              </ResWords>
            </ResWordSet>
          </ResWordSets>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>numbers</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>([0-9]+\.[0-9]*(e|E)(\+|\-)?[0-9]+)|([0-9]+\.[0-9]*)|([0-9]+)</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>strings</Name>
          <LexStyle>5</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>("")|"(([^"]*")|([^"]*))</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
  </States>
</LexScheme>