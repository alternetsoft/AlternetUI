<?xml version="1.0" encoding="utf-8"?>
<LexScheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Author>AlterNET Software</Author>
  <Name>Ini files</Name>
  <Desc>Syntax Scheme for Ini files</Desc>
  <Copyright>Copyright AlterNET Software, 2016-2019</Copyright>
  <FileExtension>.ini</FileExtension>
  <FileType>ini</FileType>
  <Version>1.5</Version>
  <Styles>
    <Style>
      <Name>idents</Name>
      <ForeColor>Teal</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>comments</Name>
      <ForeColor>Gray</ForeColor>
      <FontStyle>Italic</FontStyle>
      <PlainText>true</PlainText>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>whitespace</Name>
      <ForeColor>Gray</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>sections</Name>
      <ForeColor>Blue</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
    <Style>
      <Name>values</Name>
      <ForeColor>ControlText</ForeColor>
      <ForeColorEnabled>true</ForeColorEnabled>
      <BackColorEnabled>true</BackColorEnabled>
      <BoldEnabled>true</BoldEnabled>
      <ItalicEnabled>true</ItalicEnabled>
      <UnderlineEnabled>true</UnderlineEnabled>
    </Style>
  </Styles>
  <States>
    <State>
      <Name>normal</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>comments</Name>
          <LexStyle>1</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>\;.*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>idents</Name>
          <LexStyle>0</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>[a-zA-Z_][a-zA-Z0-9_]*</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>sections</Name>
          <LexStyle>3</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\[\])|\[(([^\]]*\])|([^\]]*))</string>
            <string>("")|"(([^"]*")|([^"]*))</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>equal</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>1</LeaveState>
          <Expressions>
            <string>=</string>
          </Expressions>
        </SyntaxBlock>
        <SyntaxBlock>
          <Name>whitespace</Name>
          <LexStyle>2</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>(\s)+</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
    <State>
      <Name>values</Name>
      <CaseSensitive>false</CaseSensitive>
      <SyntaxBlocks>
        <SyntaxBlock>
          <Name>values</Name>
          <LexStyle>4</LexStyle>
          <LeaveState>0</LeaveState>
          <Expressions>
            <string>.*</string>
          </Expressions>
        </SyntaxBlock>
      </SyntaxBlocks>
    </State>
  </States>
</LexScheme>